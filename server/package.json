{
	"name": "image-gallery-backend",
	"version": "1.0.0",
	"description": "Backend API for secure image gallery with comprehensive history tracking",
	"main": "dist/server.js",
	"scripts": {
		"dev": "nodemon src/server.ts",
		"build": "npx tsc && npx prisma generate",
		"start": "node dist/server.js",
		"prod": "npm run build && npm start",
		"db:migrate": "npx prisma migrate deploy",
		"db:clear": "npx ts-node scripts/clearDatabase.ts",
		"db:reset": "npm run db:clear && npx prisma migrate reset --force",
		"uploads:clear": "powershell \"Remove-Item -Path 'uploads/*' -Force -ErrorAction SilentlyContinue\"",
		"reset:all": "npm run db:clear && npm run uploads:clear"
	},
	"keywords": [
		"image-gallery",
		"typescript",
		"express",
		"prisma",
		"docker"
	],
	"author": "",
	"license": "MIT",
	"dependencies": {
		"@prisma/client": "^6.16.1",
		"@types/bcrypt": "^6.0.0",
		"@types/express": "^5.0.3",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/multer": "^2.0.0",
		"@types/node": "^24.5.0",
		"bcrypt": "^6.0.0",
		"cookie-parser": "^1.4.7",
		"cors": "^2.8.5",
		"crypto-js": "^4.2.0",
		"dotenv": "^17.2.2",
		"express": "^5.1.0",
		"jsonwebtoken": "^9.0.2",
		"morgan": "^1.10.1",
		"multer": "^2.0.2",
		"nodemon": "^3.1.10",
		"prisma": "^6.16.1",
		"sharp": "^0.34.3",
		"ts-node": "^10.9.2",
		"typescript": "^5.9.2",
		"uuid": "^13.0.0",
		"zod": "^4.1.8"
	},
	"devDependencies": {
		"@types/cookie-parser": "^1.4.9",
		"@types/cors": "^2.8.19",
		"@types/crypto-js": "^4.2.2",
		"@types/morgan": "^1.9.10"
	}
}
